{
  "name": "blog",
  "version": "1.0.0",
  "description": "My ultimate Nuxt.js project",
  "author": "weilei",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=development nodemon server/index.ts --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production ts-node -T server/index.ts",
    "generate": "nuxt generate",
    "lint": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "test": "ava",
    "pulish": "nuxt build && npm start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "ava": {
    "files": [
      "test/**/*"
    ]
  },
  "dependencies": {
    "@nuxtjs/axios": "5.6.0",
    "@nuxtjs/vuetify": "1.10.2",
    "cross-env": "5.2.1",
    "dayjs": "1.8.19",
    "iview": "3.5.4",
    "koa": "2.8.1",
    "koa-router": "7.4.0",
    "nuxt": "2.11.0",
    "sha256": "0.2.0",
    "typescript": "3.7.5",
    "vue-property-decorator": "8.2.2"
  },
  "devDependencies": {
    "@nuxt/types": "0.6.1",
    "@nuxt/typescript-build": "0.5.6",
    "@nuxtjs/eslint-config-typescript": "1.0.0",
    "@nuxtjs/eslint-module": "1.1.0",
    "@types/koa": "2.0.50",
    "@types/sha256": "0.2.0",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "ava": "2.4.0",
    "babel-eslint": "10.0.3",
    "babel-plugin-module-resolver": "3.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-nuxt": "0.5.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "2.7.0",
    "jsdom": "15.1.1",
    "lint-staged": "8.2.1",
    "nodemon": "1.19.2",
    "prettier": "1.18.2",
    "require-extension-hooks": "0.3.3",
    "require-extension-hooks-babel": "1.0.0",
    "require-extension-hooks-vue": "3.0.0",
    "stylus": "0.54.7",
    "stylus-loader": "3.0.2",
    "ts-node": "8.4.1"
  }
}
